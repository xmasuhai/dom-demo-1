olumn":8},"source":"main.js","original":{"line":65,"column":12}},{"name":"removeEventListener","generated":{"line":74,"column":9},"source":"main.js","original":{"line":65,"column":13}},{"generated":{"line":74,"column":28},"source":"main.js","original":{"line":65,"column":8}},{"generated":{"line":74,"column":29},"source":"main.js","original":{"line":65,"column":33}},{"generated":{"line":74,"column":40},"source":"main.js","original":{"line":65,"column":8}},{"name":"addFn","generated":{"line":74,"column":42},"source":"main.js","original":{"line":65,"column":46}},{"generated":{"line":74,"column":47},"source":"main.js","original":{"line":65,"column":8}},{"generated":{"line":75,"column":0},"source":"main.js","original":{"line":66,"column":5}},{"generated":{"line":75,"column":3},"source":"main.js","original":{"line":63,"column":4}},{"generated":{"line":76,"column":0},"source":"main.js","original":{"line":67,"column":1}},{"generated":{"line":76,"column":1},"source":"main.js","original":{"line":60,"column":0}}],"sources":{"main.js":"/* 对比 document.createElement(\"div\") 简化代码\n ** const div = dom.create(\"div\")\n */\n/* 直接写出 HTML 结构 */\nconst div = dom.create(\"<div><span id='newDiv'>newDiv</span></div>\");\nconst td = dom.create(\"<tr><td>TD</td></tr>\");\nconsole.log(div);\nconsole.log(td);\n\n/* after */\ndom.after(test, div);\nconst div3 = dom.create('<div id=\"wrapper\">DIV3</div>'); // 父节点\ndom.wrap(test, div3);\n/* empty test */\nconst nodes = dom.empty(window.empty);\nconsole.log(nodes);\n\n/* 改 */\n/* 用于读写属性  attr(node, attributeName, value) */\ndom.attr(test, \"title\", \"Hi, Jack\");\nconst title = dom.attr(test, \"title\");\nconsole.log(`title: ${title}`);\n/* 用于读写文本内容 */\ndom.text(test, \"Hello,this is a new text\");\n\n/* 用于读写HTML内容 */\nconst Dad = dom.html(dad);\nconsole.log(`Dad: ${Dad}`);\n\n/* 用于修改style */\ndom.style(test, {\n    border: \"1px solid cyan\",\n    color: \"red\"\n});\nconsole.log(dom.style(test, \"border\"));\n/* 重载 修改 style */\ndom.style(test, \"border\", \"5px solid olive\");\n/* 重载 读取 style */\nconst styleBorder = dom.style(test, \"border\");\nconsole.log(styleBorder);\n/* 用于添加class */\ndom.class.add(test, \"bgColor\");\ndom.class.add(test, \"ftSize\");\ndom.class.remove(test, \"ftSize\");\nconsole.log(dom.class.has(test, \"bgColor\"));\nconsole.log(dom.class.has(test, \"ftSize\"));\n/* 用于添加事件监听 */\nfunction addFn() {\n    console.log('指到这里')\n    console.log(`点击 ${newDiv} 取消事件`)\n    console.log(newDiv)\n}\n\n// test.addEventListener('click') // TL,DR\ndom.on(test, \"mouseenter\", addFn);\n/* 用于删除事件监听 */\n// test.removeEventListener(eventName, fn) // TL,DR\ndom.off(test, \"click\", addFn);\n\ntest.addEventListener(\"mousedown\", function() {\n    console.log(\"鼠标按下了\");\n    test.addEventListener(\"mousemove\", addFn);\n    test.addEventListener(\"mouseup\", function() {\n        console.log(\"鼠标抬起了\");\n        test.removeEventListener(\"mousemove\", addFn)\n    })\n});"},"lineCount":null}},"error":null,"hash":"0ab4b7656cf1236a18656a32e33012c1","cacheData":{"env":{}}}